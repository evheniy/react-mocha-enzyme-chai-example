{
  "extends": "airbnb",
  "env": {
	"node": true,
	"mocha": true
  },
  "parser": "babel-eslint",
  "rules": {
	  "jsx-a11y/href-no-hash": 0,
	  "no-unused-expressions": 0,
	  "no-multi-assign": 0,
    "react/jsx-filename-extension": 0,
    "react/prefer-stateless-function": 0,
    "react/jsx-no-bind": 0,
    "no-undef": 0,
    "comma-dangle": 0
  }
}
